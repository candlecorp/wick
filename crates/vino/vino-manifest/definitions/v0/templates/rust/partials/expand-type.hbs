
{{#switch kind ~}}
  {{#case "Named"}}
    {{#switch name.value}}
      {{#case "struct"}}serde_json::Value{{/case}}
      {{#case "string"}}String{{/case}}
      {{#default}}{{name.value}}{{/default}}
    {{/switch}}
  {{/case}}
  {{#case "MapType"}}
    HashMap<{{> expand-type keyType}}, {{> expand-type valueType}}>
  {{/case}}
  {{#case "ListType"}}
    Vec<{{> expand-type type}}>
  {{/case}}
  {{#case "Optional"}}
    Option<{{>expand-type type}}>
  {{/case}}
  {{#default}}
  unknown
  {{/default}}
{{~/switch}}